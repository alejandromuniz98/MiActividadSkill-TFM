{
  "type": "APL",
  "version": "1.5",
  "theme": "dark",
  "import": [
    {
      "name": "alexa-layouts",
      "version": "1.4.0"
    }
  ],
  "mainTemplate": {
    "parameters": [
      "Data"
    ],
    "items": [
      {
        "type": "Container",
        "height": "100vh",
        "width": "100vw",
        "items": [
          {
            "type": "AlexaBackground",
            "backgroundImageSource": "https://melelices.com/wp-content/uploads/2019/09/educaci%C3%B3n-f%C3%ADsica-importante.png",
            "colorOverlay": true
          },
          {
            "headerTitle": "Actividad",
            "type": "AlexaHeader",
            "headerBackButton": false,
            "headerAttributionImage": "https://i.ibb.co/f4Z42JV/icon-512-A2-Z.png"
          },
          {
        "type": "Container",
        "height": "100vh",
        "width": "100vw",
        "alignItems": "center",
            "items": [
            {
                "type": "AlexaImageListItem",
                "hideOrdinal": true,
                "secondaryText": "${Data.secondaryText}",
                "primaryText": "${Data.primaryText}",
                "imageSource": "${Data.imageSource}"
            }
            ]
          }
        ]
      }
    ]
  }
}